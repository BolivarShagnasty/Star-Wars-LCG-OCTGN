<?xml version="1.0" encoding="utf-8" ?>
   <templates>
      <blocks>
   <!-- Default Block-->
         <block type="text" id="default_name">
            <location x="85" y="100" />
            <text color="#000000" size="25" />
            <border color="#440044" size="1" />
         </block>
   <!-- Force Sphere Block-->
         <block type="overlay" id="force_sphere" src="Proxy/Elements/Force_Sphere.png">         
           <location x="20" y="65" />
         </block>  
         <block type="text" id="force_count">
           <location x="26" y="69" />
           <text color="#FFFFFF" size="13" />
           <border color="#000000" size="2" />
         </block>
   <!-- Battle Icons Block-->
         <block type="overlay" id="combat_icons_frame" src="Proxy/Elements/Icons_frame.png">         
           <location x="54" y="240" />
         </block>  
         <block type="text" id="combat_icons">
           <location x="60" y="250" />
           <text color="#000000" size="11" />
         </block>
   <!-- Generic Blocks-->
         <block type="text" id="cost">
           <location x="19" y="22" />
           <text color="#FFFFFF" size="20" />
           <border color="#FFFFFF" size="1" />
         </block>
   <!-- Event Blocks-->
         <block type="text" id="event_name">
           <location x="55" y="291" />
           <text color="#000000" size="16" />
           <wordwrap height="20" width="230" />
         </block>
         <block type="text" id="event_traits">
           <location x="56" y="328" />
           <text color="#000000" size="13" />
         </block>
         <block type="text" id="event_text">
           <location x="20" y="345" />
           <text color="#000000" size="11" />
           <wordwrap height="46" width="292" />
         </block>
         <block type="text" id="event_block">
           <location x="232" y="422" />
           <text color="#000000" size="11" />
         </block>
         <block type="text" id="event_block_number">
           <location x="288" y="422" />
           <text color="#000000" size="13" />
         </block>
   <!-- Unit Blocks-->
         <block type="text" id="unit_name">
           <location x="70" y="25" />
           <text color="#000000" size="16" />
           <wordwrap height="20" width="230" />
         </block>
         <block type="text" id="unit_traits">
           <location x="50" y="280" />
           <text color="#000000" size="13" />
         </block>
         <block type="text" id="unit_text">
           <location x="20" y="295" />
           <text color="#000000" size="11" />
           <wordwrap height="100" width="292" />
         </block>
         <block type="text" id="unit_block">
           <location x="234" y="422" />
           <text color="#000000" size="11" />
         </block>
         <block type="text" id="unit_health">
           <location x="18" y="411" />
           <text color="#FFFFFF" size="19" />
           <border color="#FFFFFF" size="2" />
         </block>
         <block type="text" id="unit_block_number">
           <location x="288" y="422" />
           <text color="#000000" size="13" />
         </block>
      </blocks>
<!-- TEMPLATES -->
      <template src="Proxy/White_Card_Template.png" default="True">
         <matches>
            <match name="Name" value="Whetever" />
         </matches>
         <textblocks>
            <link block="default_name" separator=" ">
               <property name="Name"  />
            </link>
         </textblocks>
      </template>
   <!-- Event Templates -->
      <template src="Proxy/Events/Event_Sith.png">
         <matches>
            <match name="Affiliation" value="Sith" />
            <match name="Type" value="Event" />
         </matches>
         <overlayblocks>
            <link block="force_sphere" />
         </overlayblocks>
         <textblocks>
            <link block="force_count" separator=" ">
               <property name="Force"  />
            </link>
            <link block="event_name" separator=" ">
               <property name="Name"  />
            </link>
            <link block="cost" separator=" ">
               <property name="Cost"  />
            </link>
            <link block="event_traits" separator=" ">
               <property name="Traits"  />
            </link>
            <link block="event_text" separator=" ">
               <property name="Text"  />
            </link>
            <link block="event_block" separator=" ">
               <property name="Block"  />
            </link>
            <link block="event_block_number" separator=" ">
               <property name="Block_Number"  />
            </link>
         </textblocks>
      </template>
